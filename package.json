{
  "name": "rustts",
  "author": "Nicolas Assouad",
  "license": "MIT",
  "version": "1.0.0",
  "description": "Run Typescript code faster with Rust and WebAssembly",
  "type": "module",
  "bin": "dist/index.js",
  "files": [
    "dist/index.js"
  ],
  "repository": {
    "type": "git",
    "url": "git+https://github.com/fondation451/rustts.git"
  },
  "bugs": {
    "url": "https://github.com/fondation451/rustts/issues"
  },
  "homepage": "https://github.com/fondation451/rustts#README",
  "keywords": [
    "rust",
    "ts",
    "compiler",
    "WebAssembly",
    "wasm",
    "fast"
  ],
  "scripts": {
    "build": "pnpm install && pnpm compile",
    "compile": "tsc && pnpm rollup && rimraf -rf ./dist && mkdir dist && mv index.js dist/index.js && chmod +x dist/index.js",
    "clean": "rimraf -rf ./node_modules",
    "lint": "eslint 'src/**/*.{js,ts,tsx}' --quiet",
    "prepublish": "pnpm build && pnpm quality",
    "quality": "pnpm lint && pnpm build && pnpm test",
    "rollup": "rollup --config rollup.config.js",
    "test": "node dist/index.js"
  },
  "dependencies": {
    "wasm-pack": "^0.12.1"
  },
  "devDependencies": {
    "@typescript-eslint/eslint-plugin": "^7.1.1",
    "@typescript-eslint/parser": "^7.1.1",
    "@types/lodash": "^4.17.0",
    "@types/node": "^20.11.30",
    "eslint": "^8.57.0",
    "eslint-config-next": "14.1.3",
    "eslint-config-prettier": "^9.1.0",
    "eslint-plugin-import": "^2.29.1",
    "eslint-plugin-prettier": "^5.1.3",
    "lodash": "^4.17.21",
    "prettier": "^3.2.5",
    "rimraf": "^5.0.5",
    "rollup": "^4.13.2",
    "rollup-plugin-dts": "^6.1.0",
    "typescript": "^5.4.3"
  },
  "engines": {
    "node": ">= 4.9.1"
  }
}
